<?xml version='1.94' encoding='utf-8'?>

<widget android-versionCode="10034" id="io.nardus.ica_citrus_mrl" version="1.94" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="phonegap-version" value="cli-9.0.0"/>
<icon src="img/logo1_1.png" width="1024" height="1024" />
    <icon density="mdpi" height="57" platform="android" src="img/logo.png" width="57"/>
    <name>Citrus MRL</name>
    <description>
        ICA citrus post-harvest fungicide choices for different export markets.
    </description>
    <author email="serfonteinnardus@gmail.com">
        Nardus Serfontein
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    
    <platform name="ios" version='1.94' ios-CFBundleVersion="1.94">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        
        <icon src="images/Icon-180.png" width="180" height="180" />
        <!-- iOS 7.0+ -->
        <!-- iPhone / iPod Touch  -->
        <icon src="images/Icon-Notification@3x.png" width="60" height="60" />
        <icon src="images/Icon-120.png" width="120" height="120" />
        <!-- iPad -->
        <icon src="images/Icon-76.png" width="76" height="76" />
        <icon src="images/Icon-152.png" width="152" height="152" />
        <!-- Spotlight Icon -->
        <icon src="images/Icon-Notification@2x.png" width="40" height="40" />
        <icon src="images/Icon-Small-80.png" width="80" height="80" />
        <!-- iOS 6.1 -->
        <!-- iPhone / iPod Touch -->
        <icon src="images/Icon.png" width="57" height="57" />
        <icon src="images/Icon@2x.png" width="114" height="114" />
        <!-- iPad -->
        <icon src="images/Icon-72.png" width="72" height="72" />
        <icon src="images/Icon-144.png" width="144" height="144" />
        <!-- iPad Pro -->
        <icon src="images/Icon-167.png" width="167" height="167" />
        <!-- iPhone Spotlight and Settings Icon -->
        <icon src="images/Icon-Small.png" width="29" height="29" />
        <icon src="images/Icon-Small@2x.png" width="58" height="58" />
        <!-- iPad Spotlight and Settings Icon -->
        <icon src="images/Icon-App-40x40@2x.png" width="50" height="50" />
        <icon src="images/Icon-App-40x40@2x.png" width="100" height="100" />
        <!-- iPad Pro -->
        <icon src="images/Icon-167.png" width="167" height="167" />
        
        
    </platform>
    
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-dialogs" spec="^2.0.1" />
</widget>
